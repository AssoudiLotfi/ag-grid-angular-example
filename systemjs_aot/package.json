{
  "name": "ag-grid-angular-example",
  "version": "13.3.0",
  "description": "ag-Grid Angular Example Using SystemJS",
  "scripts": {
    "postinstall": "webdriver-manager update",
    "tsc": "tsc -p tsconfig.json",
    "tsc:w": "tsc -p tsconfig.json -w",
    "lite": "lite-server",
    "clean": "npm run clean:jit && npm run clean:aot",
    "clean:jit": "rimraf app/*.js*",
    "start": "npm run tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "build":"npm run clean-build:aot",
    "clean:aot": "gulp clean-aot && gulp copy-aot-deps",
    "build:aot": "ngc -p tsconfig-aot.json && gulp aot-bundle",
    "lite:aot": "http-server ./aot/",
    "clean-build:aot": "npm run clean:aot && npm run build:aot",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "e2e-win": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.win.js\" --kill-others --success first"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ag-grid/ag-grid-angular-example.git"
  },
  "author": "Niall Crosby <niall.crosby@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ag-grid/ag-grid-angular-example/issues"
  },
  "homepage": "http://www.ag-grid.com/",
  "dependencies": {
    "ag-grid": "15.0.0",
    "ag-grid-enterprise": "15.0.0",
    "ag-grid-angular": "15.0.0",
    "@angular/common": "5.1.1",
    "@angular/compiler": "5.1.1",
    "@angular/compiler-cli": "5.1.1",
    "@angular/core": "5.1.1",
    "@angular/forms": "5.1.1",
    "@angular/http": "5.1.1",
    "@angular/platform-browser": "5.1.1",
    "@angular/platform-browser-dynamic": "5.1.1",
    "@angular/platform-server": "5.1.1",
    "@angular/animations@5.1.1 ": "5.1.1",
    "@angular/router": "5.1.1",
    "core-js": "2.5.3",
    "rxjs": "5.5.5",
    "zone.js": "0.8.18",
    "bootstrap": "3.3.7",
    "systemjs": "0.19.x"
  },
  "devDependencies": {
    "@types/node": "6.0.45",
    "@types/selenium-webdriver": "2.53.33",
    "canonical-path": "0.0.2",
    "concurrently": "2.2.0",
    "copyfiles": "1.0.x",
    "del": "2.2.x",
    "gulp": "3.9.1",
    "gulp-ngc": "0.2.x",
    "http-server": "0.9.x",
    "jasmine-core": "2.4.1",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-jasmine": "1.0.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-systemjs": "0.16.0",
    "lite-server": "2.2.2",
    "ncp": "2.0.x",
    "protractor": "5.1.2",
    "rimraf": "2.5.x",
    "run-sequence": "1.2.x",
    "systemjs-builder": "0.15.35",
    "typescript": "2.4.x",
    "webdriver-manager": "10.2.5"
  }
}
